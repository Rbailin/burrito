<h1>Sign Up!</h1>

<%=form_for @user do |u| %>
<%=u.text_field :username, :placeholder => "Enter Your Username" %><br>
<%=u.password_field :password, :placeholder => "Enter Your Password" %><br>
<%=u.submit "Create New User" %>

<% end %> 

<%=button_to "Back", {:controller => 'users', :action => 'index'}, {:method => :get} %>


